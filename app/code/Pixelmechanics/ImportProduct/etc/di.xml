*/-->
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
	<preference for="Pixelmechanics\ImportProduct\Api\PostManagementInterface" type="Pixelmechanics\ImportProduct\Model\Api\PostManagement"/>
        <type name="Pixelmechanics\ImportProduct\Logger\Logger">
        <arguments>
            <argument name="name" xsi:type="string">LogHandler</argument>
            <argument name="handlers"  xsi:type="array">
                <item name="system" xsi:type="object">Pixelmechanics\ImportProduct\Logger\Handler</item>
            </argument>
        </arguments>
    </type>
    <!-- siehe https://trello.com/c/ds9h7Vo1/34-za-api-anbindung-navision-navision-entwicklung-teil-2a-import-produkt-qty -->
    <type name="Magento\Framework\Console\CommandList">
        <arguments>
            <argument name="commands" xsi:type="array">
                <item name="importQty" xsi:type="object">Pixelmechanics\ImportProduct\Console\importQty</item>
            </argument>
        </arguments>
    </type>
</config>

