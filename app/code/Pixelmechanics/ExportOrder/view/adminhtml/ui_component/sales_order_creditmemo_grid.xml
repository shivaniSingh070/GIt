<?xml version="1.0" encoding="UTF-8"?>
<!--/**
* @author : AY
* @date : 25.10.2021
* @description : Add the Export Creditmemo dropdown in the creditmemo grid.
* @Trello: https://trello.com/c/5dHZOWuA/208-schnitt-stellen-erweiterung-retouren-info-auch-in-altem-trello-ticket
*/-->
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <listingToolbar name="listing_top">
        <massaction name="listing_massaction" component="Magento_Ui/js/grid/tree-massactions">
            <action name="orderexport">
                <settings>
                    <url path="exportorder/creditmemo/export"/>
                    <type>creditmemoexport</type>
                    <label translate="true">Export</label>
                </settings>
            </action>
        </massaction>
    </listingToolbar>
</listing>
